#!/usr/bin/env ruby
begin
  require 'rubygems'
rescue LoadError
end

# If someone calls this script directly, assuming it is a gem binstub, just nod our heads.
if ARGV[0] && ARGV[0][0,1] == '_'
  ARGV.shift
end
$LOAD_PATH.push(File.expand_path("../lib", File.dirname(__FILE__)))
require 'engineyard-serverside'

EY::Serverside::CLI.start
